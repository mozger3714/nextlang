generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(cuid())
  email       String   @unique
  clerkId     String   @unique
  stripeId    String?  @unique
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
